{
  "name": "junit",
  "version": "1.4.0",
  "description": "A simple promise based function for unit tests.",
  "main": "lib/index.js",
  "bin": {
    "junit": "bin/junit.js",
    "junit-debug": "bin/junit-debug.js"
  },
  "scripts": {
    "no": "no",
    "test": "no test",
    "prepublish": "no build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ysmood/junit.git"
  },
  "keywords": [
    "unit",
    "test",
    "promise",
    "async",
    "sync"
  ],
  "author": "http://ysmood.org",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ysmood/junit/issues"
  },
  "homepage": "https://github.com/ysmood/junit#readme",
  "files": [
    "bin",
    "lib"
  ],
  "dependencies": {
    "commander": "2.9.0",
    "nofs": "0.11.10",
    "yaku": "0.15.2"
  },
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-core": "6.5.2",
    "babel-eslint": "5.0.0",
    "babel-istanbul": "0.6.0",
    "babel-loader": "6.2.3",
    "babel-polyfill": "6.5.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-stage-2": "6.5.0",
    "coveralls": "2.11.9",
    "eslint": "2.8.0",
    "estraverse-fb": "^1.3.1",
    "nokit": "0.20.8",
    "phantomjs-prebuilt": "2.1.7",
    "webpack": "1.13.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "rules": {
      "indent": [
        2,
        4
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-trailing-spaces": 2,
      "space-before-function-paren": [
        2,
        "always"
      ],
      "eqeqeq": [
        2,
        "allow-null"
      ],
      "no-console": [
        0
      ]
    },
    "env": {
      "browser": true,
      "node": true
    },
    "extends": "eslint:recommended"
  }
}
