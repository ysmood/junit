{
  "name": "junit",
  "version": "1.1.0",
  "description": "A simple promise based function for unit tests.",
  "main": "lib/index.js",
  "bin": {
    "junit": "bin/junit.js"
  },
  "scripts": {
    "no": "no",
    "test": "no test",
    "prepublish": "no build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ysmood/junit.git"
  },
  "keywords": [
    "unit",
    "test",
    "promise",
    "async",
    "sync"
  ],
  "author": "http://ysmood.org",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ysmood/junit/issues"
  },
  "homepage": "https://github.com/ysmood/junit#readme",
  "files": [
    "bin",
    "lib"
  ],
  "dependencies": {
    "commander": "2.9.0",
    "nofs": "0.10.9",
    "yaku": "0.11.4"
  },
  "devDependencies": {
    "babel-cli": "6.1.18",
    "babel-core": "6.1.21",
    "babel-eslint": "4.1.5",
    "babel-istanbul": "0.5.8",
    "babel-loader": "6.1.0",
    "babel-polyfill": "6.1.19",
    "babel-preset-es2015": "6.1.18",
    "babel-preset-stage-2": "6.1.18",
    "coveralls": "2.11.4",
    "eslint": "1.9.0",
    "nokit": "0.16.2",
    "webpack": "1.12.6"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "rules": {
      "indent": [
        2,
        4
      ],
      "quotes": [
        2,
        "double"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-trailing-spaces": 2,
      "space-before-function-paren": [
        2,
        "always"
      ],
      "eqeqeq": [
        2,
        "allow-null"
      ],
      "no-console": [
        0
      ]
    },
    "env": {
      "browser": true,
      "node": true
    },
    "extends": "eslint:recommended"
  }
}
