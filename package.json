{
  "name": "junit",
  "version": "0.5.1",
  "description": "A simple promise based function for unit tests.",
  "main": "lib/index.js",
  "bin": {
    "junit": "bin/junit.js"
  },
  "scripts": {
    "no": "no",
    "test": "no test",
    "prepublish": "no build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ysmood/junit.git"
  },
  "keywords": [
    "unit",
    "test",
    "promise",
    "async",
    "sync"
  ],
  "author": "http://ysmood.org",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ysmood/junit/issues"
  },
  "homepage": "https://github.com/ysmood/junit#readme",
  "files": [
    "bin", "lib"
  ],
  "dependencies": {
    "yaku": "0.7.5"
  },
  "devDependencies": {
    "babel": "5.8.23",
    "babel-eslint": "4.1.2",
    "babel-istanbul": "0.3.20",
    "babel-loader": "5.3.2",
    "babel-core": "5.8.24",
    "coveralls": "2.11.4",
    "eslint": "1.4.3",
    "nokit": "0.14.1",
    "webpack": "1.12.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "rules": {
      "indent": [
        2,
        4
      ],
      "quotes": [
        2,
        "double"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-trailing-spaces": 2,
      "space-before-function-paren": [
        2,
        "always"
      ],
      "eqeqeq": [
        2,
        "allow-null"
      ],
      "no-console": [
        0
      ]
    },
    "env": {
      "browser": true,
      "node": true
    },
    "extends": "eslint:recommended"
  }
}
